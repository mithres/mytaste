<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	
	<package name="vod" extends="default" namespace="/vod">
		
		<action name="playListIndex" class="com.vc.presentation.vod.action.PlayListIndexAction">
			<result>/WEB-INF/pages/vod/playlist.jsp</result>
		</action>
		
		<action name="newPlayList" class="com.vc.presentation.vod.action.NewPlayListAction">
			<result>/WEB-INF/pages/vod/addplaylist.jsp</result>
		</action>
		
		<action name="savePlayList" class="com.vc.presentation.vod.action.SavePlayListAction">
			<result type="redirect">/vod/playListIndex</result>
		</action>
		
		<!-- Only for test -->
		
		<action name="getInfo" method="getInfo" class="com.vc.presentation.vod.action.PlayListIndexAction"/>
		
		<action name="getS" method="getS" class="com.vc.presentation.vod.action.PlayListIndexAction"/>
			
	</package>
	
	
</struts>