<?xml version="1.0" encoding="UTF-8"?>
<tc:tc-config xsi:schemaLocation="http://www.terracotta.org/schema/terracotta-4.xsd" xmlns:tc="http://www.terracotta.org/config" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <servers>
    <!--Shows where the Terracotta server can be found.-->
    <server host="172.0.2.193">
      <data>%(user.home)/terracotta/server-data</data>
      <logs>%(user.home)/terracotta/server-logs</logs>
    </server>
  </servers>
  <!--Shows where to put the generated client logs-->
  <clients>
    <logs>%(user.home)/terracotta/client-logs</logs>
    <modules>
      <repository>/home/ammen/Terracotta/modules</repository>
      <module group-id="org.terracotta.modules" name="tim-tomcat-6.0" version="2.1.0"/>
      <module group-id="org.terracotta.modules" name="tim-spring-security-2.0" version="1.3.0"/>
      <module group-id="org.terracotta.modules" name="tim-hibernate-entity-3.2" version="1.5.0"/>
      <module group-id="org.terracotta.modules" name="tim-hibernate-cache-3.2" version="1.1.0"/>
    </modules>
  </clients>
  <application>
    <dso>
      <instrumented-classes>
        <include>
          <class-expression>com.vc.entity.*</class-expression>
        </include>
        <include>
          <class-expression>com.vc.vo.ClientVO</class-expression>
        </include>
      </instrumented-classes>
      <web-applications>
        <web-application>VideoConference</web-application>
      </web-applications>
      <roots>
        <root>
          <field-name>com.vc.service.cluster.ClientManager.CLIENT_LIST</field-name>
        </root>
      </roots>
      <locks>
        <autolock auto-synchronized="true">
          <method-expression>void com.vc.vo.ClientVO.setClientKey(java.lang.String)</method-expression>
          <lock-level>write</lock-level>
        </autolock>
        <autolock auto-synchronized="true">
          <method-expression>void com.vc.vo.ClientVO.setClientID(java.lang.String)</method-expression>
          <lock-level>write</lock-level>
        </autolock>
        <autolock auto-synchronized="true">
          <method-expression>void com.vc.vo.ClientVO.setConnectSince(java.util.Date)</method-expression>
          <lock-level>write</lock-level>
        </autolock>
        <autolock auto-synchronized="true">
          <method-expression>void com.vc.vo.ClientVO.setRemoteAddress(java.lang.String)</method-expression>
          <lock-level>write</lock-level>
        </autolock>
        <autolock auto-synchronized="true">
          <method-expression>void com.vc.vo.ClientVO.setRemotePort(java.lang.Integer)</method-expression>
          <lock-level>write</lock-level>
        </autolock>
        <autolock auto-synchronized="true">
          <method-expression>void com.vc.vo.ClientVO.setScopeName(java.lang.String)</method-expression>
          <lock-level>write</lock-level>
        </autolock>
        <autolock auto-synchronized="true">
          <method-expression>void com.vc.vo.ClientVO.setSessionID(java.lang.String)</method-expression>
          <lock-level>write</lock-level>
        </autolock>
      </locks>
    </dso>
  </application>
  <tc-properties>
    <property name="session.serverId" value="vc"/>
  </tc-properties>
</tc:tc-config>